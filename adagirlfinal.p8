--explosion/bullet

function create_explosion(_x,_y)
	add(explosions, {x=_x,
																		y=_y,
																		timer=0})
																		
end

function update_explosions()
	for e in all(explosions) do
		e.timer += 1
		if e.timer == 13 then
			del(explosions, e)
		end
	end
end

function draw_explosions()
	circ(x,y,rayon, couleur)
	
	for e in all(explosions) do 
		circ(e.x, e.y, e.timer/3,
			8+e.timer%3)
	end
end
