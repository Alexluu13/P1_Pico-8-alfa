--stars

function create_stars()
 stars={}
 for i=1,20 do
  local  new_star={
   x=rnd(280),
   y=rnd(40),
   col=rnd({9,14,15}),
   speed=0.3+rnd(0.3)
  }
  add(stars, new_star)
 end
end

function update_stars()
 for s in all(stars) do 
  s.x+=s.speed
  if s.x>500 then
   s.x=280
   s.y=rnd(40)
  end
 end  
end

function draw_stars()
 for s in all(stars) do
  pset(s.x,s.y,s.col)
 end
end
