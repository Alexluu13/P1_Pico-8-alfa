--enemies

function spawn_enemies(_x)
	new_enemy={
		x=_x,
		y=88,
		life=4
	}
	add(enemies, new_enemy)
end

function update_enemies()
	for e in all(enemies) do
		e.x-=0.8
		if e.x < 0 then
			del(enemies,e)
		end
-- collision
			for b in all(bullets) do
				if collision(e,b) then
				create_explosion(b.x+4,b.y+4)
				del(bullets,b)
				e.life-=1
					if e.life==0 then 
						del(enemies,e)
					end
			end
		end
		
		if collision(p,e) then
		p.life-=1
			if p.life == 0 then
				del(p)
			end
		end	
	end
end

function update_timer_enemies()
	timer += 1
 
 if (timer % 200 == 0) then
 	if(#enemies < 10) then
 		spawn_enemies(p.x+64-8)
  end
 end
end

function draw_enemies()
	for e in all(enemies) do
  spr(25,e.x, e.y)
 end
end

function print_enemies()
print(p.life, cam_x, cam_y, 7)
end
