--bullets

function shoot()
 new_bullet={
 x=p.x,
 y=p.y,
 speed=4
 }
 add(bullets,new_bullet)
 sfx(0)
end

function update_bullets()
 for b in all(bullets) do
  b.x+=b.speed
 end
end

function draw_bullets()
 for b in all(bullets) do
 spr(7,b.x,b.y)
 end
end
