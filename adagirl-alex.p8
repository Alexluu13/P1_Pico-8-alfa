	--bullets={}
	bullets={}
end

function _update()
 movement_player()
 --jump_player()
 update_bullets()
  --timer ennemies
 update_timer_enemies()
	update_enemies()
	--colision ada and enemies
	update_explosions()
end

function _draw()
